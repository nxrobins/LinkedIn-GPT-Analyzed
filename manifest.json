{
  "manifest_version": 2,
  "name": "LinkedIn GPT Analyzer",
  "version": "1.0",
  "description": "Analyze LinkedIn profiles using GPT-4",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/in/*"],
      "js": ["content.js"]
    }
  ]
  {
  ...,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  ...
}

}
